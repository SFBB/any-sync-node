[
  {
    "label": "Run node",
    "command": "go",
    "args": [
      "run",
      "./cmd"
    ],
    "env": {
      "ANYPROF": ":8181",
      "CC": "/usr/bin/cc",
      "CXX": "/usr/bin/c++",
      "CGO_CFLAGS": "-Wno-deprecated-declarations -Wno-deprecated-non-prototype -Wno-xor-used-as-pow"
    },
    "use_new_terminal": true
  }
]
